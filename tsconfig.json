{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "outDir": "./dist",
      "rootDir": ".",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "baseUrl": "./src",
      "sourceMap": true,
      "allowJs": false,
      "noImplicitAny": false
    },
    "exclude": [
      "node_modules",
      "config.js"
    ]
  }